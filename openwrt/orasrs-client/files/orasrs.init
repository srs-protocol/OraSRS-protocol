#!/bin/sh /etc/rc.common

START=99
STOP=10

USE_PROCD=1
PROG=/usr/bin/node
APP_DIR=/opt/orasrs
APP_FILE=orasrs-simple-client.js

start_service() {
	procd_open_instance
	procd_set_param command $PROG $APP_DIR/$APP_FILE
	procd_set_param respawn
	procd_set_param stdout 1
	procd_set_param stderr 1
	procd_set_param user root
	procd_close_instance
}
