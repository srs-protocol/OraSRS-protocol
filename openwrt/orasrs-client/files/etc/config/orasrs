config orasrs 'main'
	option enabled '1'
	option api_endpoint 'https://api.orasrs.net'
	option sync_interval '3600'
	option cache_size '1000'
	option log_level 'info'
	option enable_ipv6 '1'
	option block_mode 'monitor'
	option max_memory_mb '20'
	option cache_ttl '86400'
	option timeout '10'
	option retry_times '3'
	option block_threshold '80'
	option warn_threshold '60'

config alerts 'email'
	option enabled '0'
	option smtp_server ''
	option smtp_port '587'
	option smtp_user ''
	option smtp_pass ''
	option alert_threshold '80'

config alerts 'webhook'
	option enabled '0'
	option url ''
	option alert_threshold '70'

config iot_shield 'main'
	option enabled '0'
	option shield_mode 'normal'
	option auto_block '1'
	list protected_ports '22'
	list protected_ports '23'
	list protected_ports '80'
	list protected_ports '443'
	list protected_ports '1883'
	list protected_ports '8883'
